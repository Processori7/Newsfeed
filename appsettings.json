{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "Info.log",
      "MaxFileSize": 10485760,
      "MaxFileCount": 5,
      "FormatterName": "simple",
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "AllowedHosts": "*"
}
